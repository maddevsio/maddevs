{
  "name": "maddevs",
  "version": "2.0.0",
  "description": "Mad Devs",
  "author": "Mad Devs",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "npm run lint-vue-scss & npm run lint-scss & npm run lint-es ",
    "lint-es": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint-vue-scss": "sass-lint-vue components",
    "lint-scss": "sass-lint client/assets/styles/*.scss --verbose",
    "precommit": "npm run lint",
    "heroku-postbuild": "npm run build",
    "heroku-cleanup": "cd ./client; ls | grep -v static | xargs rm -rfv",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "test": "jest --config=./jest/develop.config.js",
    "test:coverage": "jest --config=./jest/develop.config.js && node ./jest/coverageBadges.js -output './client/static'",
    "test:staging": "jest --config=./jest/staging.config.js"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.12.5",
    "@nuxtjs/prismic": "^1.2.4",
    "@nuxtjs/robots": "^2.4.2",
    "@sentry/node": "^6.2.3",
    "@sentry/tracing": "^6.2.3",
    "@sentry/vue": "^6.2.5",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.12.3",
    "nuxt": "^2.14.12",
    "prismjs": "^1.23.0",
    "request": "^2.88.2",
    "sendpulse-api": "^1.1.4",
    "simplebar-vue": "^1.6.0",
    "snyk": "^1.500.0",
    "swiper": "^6.5.1",
    "vue-awesome-swiper": "^4.1.1",
    "vue-embed-gist": "^1.1.0",
    "vue-lottie": "^0.2.1",
    "vue-prism-editor": "^1.2.2",
    "vue-select": "^3.11.2",
    "vue-social-sharing": "^3.0.7",
    "vuelidate": "^0.7.6"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/eslint-parser": "^7.13.10",
    "@babel/preset-env": "^7.11.5",
    "@testing-library/vue": "^5.6.1",
    "@vue/test-utils": "^1.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^25.5.1",
    "eslint": "^7.22.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-vue": "^7.8.0",
    "husky": "^4.3.0",
    "jest": "^25.5.4",
    "jest-canvas-mock": "^2.3.1",
    "jest-serializer-vue": "^2.0.2",
    "mkdirp": "^1.0.4",
    "node-sass": "^4.14.1",
    "regenerator-runtime": "^0.13.7",
    "sass-lint": "^1.13.1",
    "sass-lint-vue": "^0.4.0",
    "sass-loader": "^7.3.1",
    "vue-jest": "^3.0.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    }
  },
  "snyk": true
}
